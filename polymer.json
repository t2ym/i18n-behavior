{
  "entrypoint": "index.html",
  "shell": "demo/preprocess/index.js",
  "sources": [
  ],
  "extraDependencies": [
    "node_modules/intl/**",
    "node_modules/@webcomponents/webcomponentsjs/**",
    "demo/preprocess/locales/**",
    "demo/preprocess/*.json"
  ],
  "builds": [
    {
      "name": "esm-unbundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": false,
      "addServiceWorker": false
    },
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": false
    },
    {
      "name": "es6-bundled",
      "browserCapabilities": [
        "es2015"
      ],
      "js": {
        "compile": "es2015",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": false
    },
    {
      "name": "es5-bundled",
      "js": {
        "compile": "es5",
        "minify": true,
        "transformModulesToAmd": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": false
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
